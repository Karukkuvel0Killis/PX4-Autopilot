#!/bin/sh
#
# board specific sensors init
#------------------------------------------------------------------------------

board_adc start

# # Internal SPI bus BMI088 accel/gyro
# bmi088 -s -b 2 -A -R 6 start
# bmi088 -s -b 2 -G -R 6 start

# # Internal SPI bus BMI270 accel/gyro
icm20948 -s -b 1 -R 180 start


# Internal baro
# ms5611 -I start -a 0x77
# bmm150 -I start -a 0x13


ms5611 -I -b 1 -a 0x77 start
bmm150 -I -b 1 -a 0x13 start
